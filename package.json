{
  "name": "hello-diff",
  "displayName": "Hello Diff",
  "description": "Between hello world and a user-driven diff review UI that later calls external AI.",
  "version": "0.0.1",
  "publisher": "mundahlmakes",
  "engines": {
    "vscode": "^1.103.0"
  },
  "main": "./out/extension.js",
  "categories": ["Other"],
  "contributes": {
    "commands": [
      {
        "command": "hello-diff.hello",
        "title": "Hello Diff: Hello"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
